{
    "openapi": "3.0.0",
    "info": {
        "title": "RentalX Documentation",
        "description": "This is an API Rent",
        "version": "1.0.0"
    },
    "paths": {
        "/categories": {
            "post": {
                "tags": [
                    "categories"
                ],
                "summary": "Create category",
                "description": "Create a new category",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "name": {
                                        "type": "string"
                                    },
                                    "description": {
                                        "type": "string"
                                    }
                                },
                                "example": {
                                    "name": "CategoryTest",
                                    "description": "Category Test"
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Create"
                    },
                    "500": {
                        "description": "Category Already Exists"
                    }
                }
            },
            "get": {
                "tags": [
                    "categories"
                ],
                "summary": "List all categories",
                "description": "List all categories",
                "responses": {
                    "200": {
                        "description": "success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "properties": {
                                        "name": {
                                            "type": "string"
                                        },
                                        "description": {
                                            "type": "string"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/categories/import":{
            "post": {
                "tags": ["categories"],
                "summary": "upload a new Category",
                "description": "upload a new category",
                "requestBody":{
                    "content": {
                        "multipart/form-data": { 
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "file": {
                                        "type": "string",
                                        "format": "binary"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "imported"
                    }
                }
            }
        },
        "/specifications": {
            "post": {
                "tags": [
                    "Specification"
                ],
                "summary": "Create a specification",
                "description": "Creates a new specification",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/definitions/Specifications"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Created"
                    },
                    "500": {
                        "description": "Specification already exists"
                    }
                }
            }
        }
    },
    "definitions": {
        "Specifications": {
            "type": "object",
            "properties": {
                "name": {
                    "type": "string"
                },
                "description": {
                    "name": {
                        "type": "string"
                    }
                }
            }
        }
    }
}